@{
    var Students = (List<StudentModel>)ViewData["Students"];

}

<h1>
    List of all Students' table content
</h1>



<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td> Name </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Students)
        {
            <tr>
                <td> @s.Id </td>
                <td> @s.Name </td>

            </tr>
        }
    </tbody>
</table>
