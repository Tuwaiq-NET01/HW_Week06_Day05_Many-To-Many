@{
    var students = (List<Student>)ViewData["students"];
}
<h1>index student</h1>
<table class="table">

    <tr>
        <th> ID</th>
        <th>Frist Name </th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>

    <tbody>
        @foreach (var item in students)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Email</td>
            </tr>
        }

    </tbody>

</table>