@page
@{
    List<BranchModel> branches = (List<BranchModel>)ViewBag.Branches;
    @*var searchResult = ViewBag.searchResult;*@

}
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Area</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in branches)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Name</td>
                    <td>@item.Address</td>
                    <td>@item.Area</td>
                </tr>
            }
        }
    </tbody>
</table>


