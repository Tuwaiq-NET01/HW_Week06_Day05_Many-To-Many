@page
@{
    List<CustomerModel> customers = (List<CustomerModel>)ViewBag.customers;
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in customers)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Name</td>
                </tr>
            }
        }
    </tbody>
</table>