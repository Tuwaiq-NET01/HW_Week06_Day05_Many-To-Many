@{
    var firstName = (string)ViewData["First Name"];
    var IdList = (List<StudentModel>)ViewData["Id List"];
}

<h4>First Name: @firstName</h4>

<h5>Ids greater than 2</h5>

<table class="table">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>

    <tbody>
        @foreach (var Student in IdList)
        {
            <tr>
                <td>@Student.Id</td>
                <td>@Student.FirstName</td>
                <td>@Student.LastName</td>
                <td>@Student.Email</td>
            </tr>
        }
    </tbody>
</table>
