@{
    var Students = (List<StudentModel>)ViewData["Students"];
}


<h1>This is index students</h1>

<table class="table">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    <tbody>
        @foreach(var Student in Students)
            {
        <tr>
            <td>@Student.Id</td>
            <td>@Student.FirstName</td>
            <td>@Student.LastName</td>
            <td>@Student.Email</td>
            <td>
                <a href="Students/Details/?id=@Student.Id" class="text-info">Details</a>
            </td>

        </tr>
            }
    </tbody>
</table>