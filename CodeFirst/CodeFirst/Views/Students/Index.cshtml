    <thead>
        <tr>
            <td>ID</td>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Email</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var g in greaterthan2)
        {

            <tr>
                <td>@g.Id</td>
                <td>@g.FirstName</td>
                <td>@g.LastName</td>
