@{
    var Costomer = (List<CustomerModel>)ViewData["Costomers"];
}

<h1>this is the Costomers index</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @{

            foreach (var item in Costomer)
            {

                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Name</td>
                </tr>
            }
        }

    </tbody>
</table>